<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <content fileName="content.xml">
<!-- Whether to log login and receiver in the logEntries -->
    <anonymousWorkflowLog>false</anonymousWorkflowLog>
    <caseSensitiveObjectNames>true</caseSensitiveObjectNames>
    <contentService>
      <defaultPublication>publication</defaultPublication>
    </contentService>
    <generateThumbnails>true</generateThumbnails>
    <genericDocumentsCanContainImages>false</genericDocumentsCanContainImages>
    <inputCharsets type="list">
      <listitem>iso8859-1</listitem>
      <listitem>iso8859-2</listitem>
      <listitem>utf-8</listitem>
    </inputCharsets>
    <invalidAnchorTargets/>
    <keepOldVersions>true</keepOldVersions>
    <mimeTypes fileName="mimeTypes.xml">
      <pdf>application/pdf</pdf>
      <html>text/html</html>
      <jpg>image/jpeg</jpg>
      <asp>text/html</asp>
      <bz2>application/x-bzip2</bz2>
      <class>application/class</class>
      <css>text/css</css>
      <doc>application/msword</doc>
      <frame>application/vnd.framemaker</frame>
      <gif>image/gif</gif>
      <gz>application/x-gzip</gz>
      <htm>text/html</htm>
      <ico>image/x-icon</ico>
      <inc>text/html</inc>
      <jar>application/java-archive</jar>
      <java>text/plain</java>
      <jpeg>image/jpeg</jpeg>
      <js>text/plain</js>
      <jsp>text/html</jsp>
      <m4a>audio/mp4</m4a>
      <m4v>video/mp4</m4v>
      <mp4>video/mp4</mp4>
      <mp3>audio/mpeg</mp3>
      <mov>video/quicktime</mov>
      <php>text/html</php>
      <png>image/png</png>
      <pps>application/vnd.ms-powerpoint</pps>
      <ppt>application/vnd.ms-powerpoint</ppt>
      <properties>text/plain</properties>
      <rtf>application/rtf</rtf>
      <shtml>text/html</shtml>
      <stc>application/vnd.sun.xml.calc.template</stc>
      <stw>application/vnd.sun.xml.writer.template</stw>
      <swf>application/x-shockwave-flash</swf>
      <sxc>application/vnd.sun.xml.calc</sxc>
      <sxw>application/vnd.sun.xml.writer</sxw>
      <tar>application/x-gtar</tar>
      <tbz2>application/x-bzip2</tbz2>
      <templ>text/html</templ>
      <tgz>application/x-gzip</tgz>
      <tif>image/tiff</tif>
      <tiff>image/tiff</tiff>
      <txt>text/plain</txt>
      <wmf>image/wmf</wmf>
      <wml>text/html</wml>
      <xls>application/vnd.ms-excel</xls>
      <xml>text/plain</xml>
      <zip>application/zip</zip>
    </mimeTypes>
    <publicationIndexFileName>index</publicationIndexFileName>
    <sortKey>name</sortKey>
    <thumbnailSize>48</thumbnailSize>
    <validAnchorTargets/>
    <workflow>
      <editStepMembershipRequiredForEditor>false</editStepMembershipRequiredForEditor>
    </workflow>
  </content>
  <export fileName="export.xml">
    <abortOnError>false</abortOnError>
    <absoluteFsPathPrefix/>
    <absoluteUrlPrefix/>
    <charset>utf-8</charset>
    <charsetMap>
      <ascii>US-ASCII</ascii>
      <big5>Big5</big5>
      <cp437>IBM437</cp437>
      <cp775>IBM775</cp775>
      <cp850>IBM850</cp850>
      <cp852>IBM852</cp852>
      <cp855>IBM855</cp855>
      <cp857>IBM857</cp857>
      <cp860>IBM860</cp860>
      <cp861>IBM861</cp861>
      <cp862>IBM862</cp862>
      <cp863>IBM863</cp863>
      <cp864>IBM864</cp864>
      <cp865>IBM865</cp865>
      <cp866>IBM866</cp866>
      <cp869>IBM869</cp869>
      <euc-cn>GB2312</euc-cn>
      <euc-jp>EUC-JP</euc-jp>
      <euc-kr>EUC-KR</euc-kr>
      <gb1988>ISO646-CN</gb1988>
      <gb2312>GB_2312-80</gb2312>
      <iso2022-jp>ISO-2022-JP</iso2022-jp>
      <iso2022-kr>ISO-2022-KR</iso2022-kr>
      <iso8859-15>ISO-8859-15</iso8859-15>
      <iso8859-1>ISO-8859-1</iso8859-1>
      <iso8859-2>ISO-8859-2</iso8859-2>
      <iso8859-3>ISO-8859-3</iso8859-3>
      <iso8859-4>ISO-8859-4</iso8859-4>
      <iso8859-5>ISO-8859-5</iso8859-5>
      <iso8859-6>ISO-8859-6</iso8859-6>
      <iso8859-7>ISO-8859-7</iso8859-7>
      <iso8859-8>ISO-8859-8</iso8859-8>
      <iso8859-9>ISO-8859-9</iso8859-9>
      <jis0201>JIS_C6220-1969</jis0201>
      <jis0208>JIS_C6226-1983</jis0208>
      <jis0212>JIS_X0212-1990</jis0212>
      <koi8-r>KOI8-R</koi8-r>
      <ksc5601>KSC_5601</ksc5601>
      <shiftjis>Shift_JIS</shiftjis>
      <unicode>ISO-10646-UCS-2</unicode>
      <utf-8>UTF-8</utf-8>
    </charsetMap>
    <convertNpspm>
      <isActive>true</isActive>
      <mapping>
        <asp>velocity</asp>
        <htm>velocity</htm>
        <html>velocity</html>
        <inc>velocity</inc>
        <jsp>jsp</jsp>
        <php>velocity</php>
        <shtml>velocity</shtml>
        <wml>velocity</wml>
        <xml>velocity</xml>
      </mapping>
      <hideForbiddenLinks>true</hideForbiddenLinks>
    </convertNpspm>
    <defaultDoctype>html</defaultDoctype>
    <exportAbsolutePaths>false</exportAbsolutePaths>
<!-- This value can be used by scripts associated with the content in
            order to create links pointing back to the editorial system. -->
    <guiUrl>http://localhost:8080/default/NPS</guiUrl>
    <incrementalUpdate>
      <isActive>false</isActive>
      <exportBaseDir>export</exportBaseDir>
      <login>root</login>
      <password>changeme</password>
    </incrementalUpdate>
    <referenceAttributes type="list"/>
    <tclDynamicLinkFormatterCommands>
      <include>formatter:include</include>
      <phpInclude>formatter:phpInclude</phpInclude>
      <phpIncludeOnce>formatter:phpIncludeOnce</phpIncludeOnce>
      <phpRequire>formatter:phpRequire</phpRequire>
      <phpRequireOnce>formatter:phpRequireOnce</phpRequireOnce>
    </tclDynamicLinkFormatterCommands>
    <tclFormatterCommands>
      <phpVardef>::formatter::phpVardef</phpVardef>
      <formatBlobLength>::formatter::blobLength</formatBlobLength>
      <formatToRFC822>::formatter::rfc822</formatToRFC822>
      <dehtmlize>::formatter::dehtmlize</dehtmlize>
    </tclFormatterCommands>
    <tclSystemExecuteCommands>
      <editLink>systemExecute:editLink</editLink>
      <currentDate>systemExecute:currentDate</currentDate>
    </tclSystemExecuteCommands>
    <toclistSortLanguage>de_DE</toclistSortLanguage>
    <validDateTimeOutputFormats>
      <europeanDate>%d.%m.%Y</europeanDate>
      <europeanLongDateTime>%d.%m.%Y %H:%M:%S %Z (%z)</europeanLongDateTime>
      <europeanShortDateTime>%d.%m.%Y %H:%M %Z</europeanShortDateTime>
      <europeanShortDateTimeWoZone>%d.%m.%Y %H:%M</europeanShortDateTimeWoZone>
      <isoDate>%Y-%m-%d %Z</isoDate>
      <isoLongDateTime>%Y-%m-%d %H:%M:%S %Z (%z)</isoLongDateTime>
      <isoShortDateTime>%Y-%m-%d %H:%M %Z</isoShortDateTime>
      <us24hLongDateTime>%m/%d/%Y %H:%M:%S %Z (%z)</us24hLongDateTime>
      <us24hShortDateTime>%m/%d/%Y %H:%M %Z</us24hShortDateTime>
      <usAmPmLongDateTime>%m/%d/%Y %I:%M:%S %p %Z (%z)</usAmPmLongDateTime>
      <usAmPmShortDateTime>%m/%d/%Y %I:%M %p %Z</usAmPmShortDateTime>
      <usDate>%m/%d/%Y</usDate>
    </validDateTimeOutputFormats>
    <wantLinkCallback>false</wantLinkCallback>
    <writeLiveServerReadPerms>true</writeLiveServerReadPerms>
  </export>
  <gui fileName="gui.xml">
<!-- For explanations see guiConfig.format -->
<!-- auto-detect the highest available jre -->
<!-- prefer https to supress ie security warnings -->
    <jreSpecification name="auto">
      <clsId>8AD9C840-044E-11D1-B3E9-00805F499D93</clsId>
      <codebase>https://java.sun.com/products/plugin/autodl/jinstall-1_4-windows-i586.cab#Version=1,4,0,0</codebase>
    </jreSpecification>
<!-- forcing usage of a particular jre version
    <jreSpecification name="1.4.2_07">
        <clsId>CAFEEFAC-0014-0002-0007-ABCDEFFEDCBA</clsId>
        <codebase>https://java.sun.com/update/1.4.2/jinstall-1_4_2_07-windows-i586.cab#Version=1,4,2,7</codebase>
    </jreSpecification>
    -->
    <contentInspectors fileName="contentInspectors.xml">
<!-- inspectors and or views may be defined in an optional <beans> element,
         see inspectorRegistry.xml - beans defined here will be added or will
         replace the default configuration! -->
      <beans>
        <!-- tabbed overview inspectors. -->
        <!--
        <bean id="publication-overview" parent="abstract-overview">
            <property name="template" value="tabbed-overview.vm"/>
        </bean>
        <bean id="document-overview" parent="abstract-overview">
            <property name="template" value="tabbed-overview.vm"/>
        </bean>
        <bean id="image-overview" parent="abstract-overview">
            <property name="template" value="tabbed-overview.vm"/>
        </bean>
        <bean id="generic-overview" parent="abstract-overview">
            <property name="template" value="tabbed-overview.vm"/>
        </bean>
        <bean id="template-overview" parent="abstract-overview">
            <property name="template" value="tabbed-overview.vm"/>
        </bean>
        -->
