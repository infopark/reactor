    <incrementalExport>
      <isActive>false</isActive>
      <collectionSelection>
        <select collection="">
          <matches name="contentType" value="^(css|js)$"/>
        </select>
        <select collection="">
          <isEqual name="objType" value="image"/>
        </select>
        <select collection="">
          <isEqual name="mimeType" value="application/zip"/>
        </select>
        <select collection="live-docs"/>
      </collectionSelection>
    </incrementalExport>
    <staticExport>
      <isActive>false</isActive>
      <collectionSelection/>
    </staticExport>
    <vseLocale>uni</vseLocale>
  </indexing>
  <searching fileName="searching.xml">
    <postprocessor/>
    <preprocessor/>
    <searchResultFormatterCommands>
      <uppercase>uppercase</uppercase>
    </searchResultFormatterCommands>
  </searching>
  <server fileName="server.xml">
    <cm>
      <tclHost/>
      <tclPort>3001</tclPort>
      <httpHost/>
      <httpConnectHost>localhost</httpConnectHost>
      <httpPort>3002</httpPort>
      <stateHost>localhost</stateHost>
      <statePort>3003</statePort>
      <httpProxyHost/>
      <httpProxyPort/>
      <database fileName="cmdb.xml">
        <adaptor>mysql</adaptor>
        <server>db</server>
        <database>reactor_test</database>
        <user>reactor_user</user>
        <password>reactor_user</password>
        <port>3306</port>
<!-- You must NOT change the value for 'storeBlobsInDatabase' after you have
        executed the application for the first time. If you do so you *will*
        lose data. -->
        <storeBlobsInDatabase>true</storeBlobsInDatabase>
      </database>
    </cm>
    <ses>
      <tclHost/>
      <tclPort>3012</tclPort>
      <httpHost>0.0.0.0</httpHost>
      <httpConnectHost>0.0.0.0</httpConnectHost>
      <httpPort>3011</httpPort>
      <stateHost>0.0.0.0</stateHost>
      <statePort>3013</statePort>
      <httpProxyHost/>
      <httpProxyPort/>
    </ses>
    <te>
      <tclHost/>
      <tclPort>3051</tclPort>
      <httpHost/>
      <httpConnectHost>localhost</httpConnectHost>
      <httpPort>3052</httpPort>
      <stateHost>localhost</stateHost>
      <statePort>3053</statePort>
      <httpProxyHost/>
      <httpProxyPort/>
      <database fileName="tedb.xml">
        <adaptor>sqlite</adaptor>
      </database>
    </te>
    <log fileName="log.xml">
<!-- Valid "info" log levels are 0, 1, 2, 3, where each level
         includes logging the messages of all numerically lower levels
         (lower number = higher priority).
         Messages will go to the specified log file. -->
      <logger name="info" level="2" logFile="info.log"/>
      <logger name="error" logFile="error.log"/>
      <logger name="alert" logFile="error.log"/>
      <logger name="warning" logFile="error.log"/>
      <logger name="debug" logFile="debug.log"/>
    </log>
    <email>
<!-- leave 'host' empty to deactive email services -->
<!--
        <host>localhost</host>
        -->
      <host/>
    </email>
    <instanceSecret>MYSECRET</instanceSecret>
  </server>
  <tuning fileName="tuning.xml">
    <clearCachesFrequency>100</clearCachesFrequency>
    <export>
      <acceptableFailures>0</acceptableFailures>
      <maxNumberOfExportedObjects>500</maxNumberOfExportedObjects>
      <maxParallelExports>2</maxParallelExports>
      <maxRecursionLevel>100</maxRecursionLevel>
      <timeout>60</timeout>
      <usesAllThreshold>200</usesAllThreshold>
    </export>
    <indexing>
      <maxBulkSize>200</maxBulkSize>
      <interval>60</interval>
      <optimizationInterval>86400</optimizationInterval>
      <filterTimeout>300</filterTimeout>
    </indexing>
    <jobMaxLogLength>50</jobMaxLogLength>
    <master>
      <cm>
        <minIdleSlaves>0</minIdleSlaves>
        <maxSlaves>25</maxSlaves>
        <slaveExecArguments/>
      </cm>
      <ses>
        <minIdleSlaves>0</minIdleSlaves>
        <maxSlaves>25</maxSlaves>
        <slaveExecArguments/>
      </ses>
      <te>
        <minIdleSlaves>1</minIdleSlaves>
        <maxSlaves>5</maxSlaves>
        <slaveExecArguments/>
      </te>
      <slaveExecMaxFailures>4</slaveExecMaxFailures>
      <slaveIdleTimeout>3600</slaveIdleTimeout>
      <slaveShutdownTimeout>10</slaveShutdownTimeout>
      <slaveStartupTimeout>120</slaveStartupTimeout>
    </master>
    <maxSearchResultSize>0</maxSearchResultSize>
    <minStreamingDataLength>8</minStreamingDataLength>
    <slave>
      <masterTimeout>1</masterTimeout>
      <maxNumberOfRequests>400</maxNumberOfRequests>
      <clientRequestTimeout>30</clientRequestTimeout>
      <httpRequestTimeout>120</httpRequestTimeout>
