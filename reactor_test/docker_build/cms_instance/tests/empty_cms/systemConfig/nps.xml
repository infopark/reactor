<!-- templates references by content or default views may be defined in an
         optional <templates> element containing <template name="..."> elements
         with a CDATA section for each template - these will override the
         default templates! -->
    </contentInspectors>
    <contentMenu fileName="contentMenu.xml">
<!-- additional items may be defined in an optional <beans> element,
         see itemRegistry.xml -->
      <beans>
        <bean id="pdfServiceConfig" abstract="true">
          <property name="pdfDownloadUrl" value="/infopark-pdf-generator/getPdf"/>
          <property name="xslPath">
            <null/>
          </property>
          <property name="pdfTemplateName" value="mastertemplate"/>
        </bean>
        <bean id="createPdfManual" parent="pdfServiceConfig" class="com.infopark.cm.htmlgui.browse.fopper.CreateAction">
          <property name="titles">
            <map>
              <entry key="de" value="PDF-Handbuch erstellen"/>
              <entry key="en" value="Generate PDF Manual"/>
              <entry key="fr" value="Créer manuel au format PDF"/>
            </map>
          </property>
          <property name="selectionType" value="single"/>
          <property name="objectTypes">
            <set>
              <value>document</value>
              <value>publication</value>
            </set>
          </property>
          <property name="dialogSize" value="small"/>
<!--property name="pdfDebugUrl" value="/infopark-pdf-generator/getIndex"/-->
        </bean>
        <bean id="updatePdfManual" parent="pdfServiceConfig" class="com.infopark.cm.htmlgui.browse.fopper.UpdateAction">
          <property name="titles">
            <map>
              <entry key="de" value="PDF-Handbuch aktualisieren"/>
              <entry key="en" value="Update PDF Manual"/>
              <entry key="fr" value="Mettre à jour manuel au format PDF"/>
            </map>
          </property>
          <property name="selectionType" value="single"/>
          <property name="objectTypes">
            <set>
              <value>generic</value>
            </set>
          </property>
          <property name="dialogSize" value="small"/>
        </bean>
      </beans>
<!-- additional menu entries to be merged may be defined in an optional
         <menuBar> element, see WEB-INF/menuBar.xml -->
      <menuBar>
        <!--
        <menu name="file">
            <group name="object">
                <item roles="admin">createPdfManual</item>
                <item roles="admin">updatePdfManual</item>
            </group>
        </menu>
        -->
    </menuBar>
<!-- additional toolbar entries to be merged may be defined in an optional
         <toolBar> element, see WEB-INF/toolBar.xml -->
      <toolBar/>
    </contentMenu>
    <dynamicPreviewUrl/>
    <dynamicPreviewDirectory/>
    <dynamicPreviewExtensions>
        <!--
        <extension>php</extension>
        -->
    </dynamicPreviewExtensions>
    <fontFamily>
<!-- fonts which can be selected by the user -->
      <item>Arial</item>
      <item>Helvetica</item>
      <item>Tahoma</item>
      <item>Times</item>
      <item>Verdana</item>
      <item>Courier</item>
      <item>Courier New</item>
      <item>Lucida Console</item>
    </fontFamily>
    <fontSize>
<!-- font sizes which can be selected by the user -->
      <item>6pt</item>
      <item>7pt</item>
      <item>8pt</item>
      <item>9pt</item>
      <item>10pt</item>
      <item>11pt</item>
      <item>12pt</item>
      <item>14pt</item>
    </fontSize>
    <previewMasterTemplates>
      <template>
        <name>mastertemplate</name>
        <title lang="de">Standard</title>
        <title lang="en">Standard</title>
        <title lang="es">Standard</title>
        <title lang="fr">Standard</title>
        <title lang="it">Standard</title>
      </template>
      <template>
        <name>mt2</name>
        <title lang="de">Alternative</title>
        <title lang="en">Alternative</title>
        <title lang="es">Alternativo</title>
        <title lang="fr">Alternatif</title>
        <title lang="it">Alternativo</title>
      </template>
    </previewMasterTemplates>
    <roles>
      <role>
        <name>admin</name>
        <title lang="de">Volle Sicht</title>
        <title lang="en">Full View</title>
        <title lang="es">Full View</title>
        <title lang="fr">Vue complète</title>
        <title lang="it">Full View</title>
<!--
            <availableFor>
                <group>admins</group>
            </availableFor>
            -->
        <values>
          <browserName>cb</browserName>
          <enableToolBar>true</enableToolBar>
          <browserShowsInactive>true</browserShowsInactive>
          <browserShowsTitles>false</browserShowsTitles>
          <browserShowsImages>true</browserShowsImages>
          <browserShowsGenerics>true</browserShowsGenerics>
          <browserShowsTemplates>true</browserShowsTemplates>
          <browserShowsDocuments>true</browserShowsDocuments>
          <preferEditedTemplates>true</preferEditedTemplates>
          <preferEditedContents>true</preferEditedContents>
          <defaultTemplate>mastertemplate</defaultTemplate>
        </values>
      </role>
      <role>
        <name>restricted</name>
        <title lang="de">Eingeschränkte Sicht</title>
        <title lang="en">Restricted View</title>
        <title lang="es">Restricted View</title>
        <title lang="fr">Vue réduite</title>
        <title lang="it">Restricted View</title>
        <values>
          <browserName fixed="true">pb</browserName>
          <enableToolBar>true</enableToolBar>
          <browserShowsInactive fixed="true">false</browserShowsInactive>
          <browserShowsTitles>true</browserShowsTitles>
          <browserShowsImages>true</browserShowsImages>
          <browserShowsGenerics>true</browserShowsGenerics>
          <browserShowsTemplates fixed="false">false</browserShowsTemplates>
          <browserShowsDocuments>true</browserShowsDocuments>
          <preferEditedTemplates fixed="false">false</preferEditedTemplates>
          <preferEditedContents>true</preferEditedContents>
          <defaultTemplate>mastertemplate</defaultTemplate>
        </values>
      </role>
    </roles>
<!--
      Pre-6.10.3 installations may activate continued support of EOP.
      This requires to provide webapps/GUI/NPS/eop, e.g. from a previous Fiona version.
    -->
    <supportsEop>false</supportsEop>
    <webDav fileName="webDav.xml">
<!-- Publication class to use when creating a new empty "directory"
         via WebDAV. Default is "publication". -->
      <defaultPublicationClass>publication</defaultPublicationClass>
<!-- Object class to use when creating a new object from a "file"
        via WebDAV. You have to make sure that the given object class
        allows the content type in question. Default is "generic".
        -->
      <objectClasses type="dictionary">
        <html>Document</html>
        <wri>Generic</wri>
        <css>Generic</css>
        <js>Generic</js>
        <txt>Generic</txt>
        <pdf>Generic</pdf>
        <doc>Generic</doc>
        <gif>Image</gif>
        <jpg>Image</jpg>
        <jpeg>Image</jpeg>
        <png>Image</png>
        <templ>template</templ>
      </objectClasses>
      <substituteUnderscoreWithSpaces>true</substituteUnderscoreWithSpaces>
    </webDav>
  </gui>
  <indexing fileName="indexing.xml">
    <advancedSearch>
      <isActive>true</isActive>
<!-- The first matching collection wins. It is not possible to index a
            content into more than one collection at a time. -->
      <collectionSelection>
        <select collection="cm-contents">
          <isEqual name="state" value="edited"/>
        </select>
        <select collection="cm-contents">
          <isEqual name="state" value="released"/>
        </select>
      </collectionSelection>
    </advancedSearch>
    <contentPreprocessors type="list">
      <listitem>
        <mimeTypes type="list">
          <listitem>application/ami-pro</listitem>
          <listitem>application/pdf</listitem>
          <listitem>application/postscript</listitem>
          <listitem>application/rtf</listitem>
          <listitem>application/vnd.ms-works</listitem>
          <listitem>application/vnd.mif</listitem>
          <listitem>application/msword</listitem>
          <listitem>application/wordperfect5.1</listitem>
          <listitem>text/richtext</listitem>
        </mimeTypes>
        <processor type="internal"/>
      </listitem>
      <listitem>
        <mimeTypes type="list">
          <listitem>application/vnd.lotus-1-2-3</listitem>
          <listitem>application/vnd.lotus-freelance</listitem>
          <listitem>application/vnd.ms-excel</listitem>
          <listitem>application/vnd.ms-powerpoint</listitem>
          <listitem>application/vnd.ms-tnef</listitem>
          <listitem>application/x-executable</listitem>
          <listitem>application/zip</listitem>
        </mimeTypes>
        <processor type="ignoreBlob"/>
      </listitem>
<!-- example for configuring an external indexer preprocessor -->
<!--
        <listitem>
            <mimeTypes type="list">
                <listitem>application/ami-pro</listitem>
                <listitem>application/pdf</listitem>
